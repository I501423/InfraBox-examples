{
    "version": 1,
    "jobs": [{
        "type": "docker",
        "name": "parent",
        "resources": { "limits": { "cpu": 1, "memory": 1024 } },
        "build_only": false,
        "docker_file": "infrabox/parent/Dockerfile"
    }, {
        "type": "docker",
        "name": "child",
        "resources": { "limits": { "cpu": 1, "memory": 1024 } },
        "build_only": false,
        "docker_file": "infrabox/child/Dockerfile",
        "depends_on": ["parent"]
    }]
}
